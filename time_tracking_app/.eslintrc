{
    "parser": "babel-eslint",
    "extends": [
        "react-app",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "react/react-in-jsx-scope": 0,
        "prettier/prettier": [
            {
                "singleQuote": true
            }
        ]
    },
    "plugins": [
        "react",
        "import"
    ]
}